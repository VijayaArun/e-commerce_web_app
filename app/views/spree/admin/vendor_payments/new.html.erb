<%= form_for [:admin, @user, @payment], method: :post, url: admin_user_vendor_payments_path(@user.id), :html => { :remote => true} do |f| %>
  <fieldset>
    <legend align="center"><%= Spree.t("admin.vendor_payments.new") %></legend>
